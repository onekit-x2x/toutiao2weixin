<onekit-view onekit-class="container">
  <onekit-view onekit-class="body">
    <onekit-view onekit-class="page-section-title page-section-title-space">
        点击向服务器发起请求
    </onekit-view>
    <onekit-view onekit-class="btn-area">
      <onekit-button bindTap="makeRequest" type="primary" disabled="{{buttonDisabled}}">request</onekit-button>
      <onekit-button bindTap="abortRequest" type="primary" disabled="{{buttonDisabled}}">abort</onekit-button>
    </onekit-view>
  </onekit-view>
</onekit-view>