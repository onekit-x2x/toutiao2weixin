    <onekit-view onekit-class="page-section page-section-gap">
      <onekit-view onekit-class="page-section-title">自定义缩放 -- 高度设置裁切显示左下部分</onekit-view>
      <onekit-view onekit-class="page-section-ctn">
        <onekit-image onekit-class="image two-direction-fix" mode="bottom left" src="{{url}}"></onekit-image>
      </onekit-view>
    </onekit-view>
    <onekit-view onekit-class="page-section page-section-gap">
      <onekit-view onekit-class="page-section-title">自定义缩放 -- 高度设置裁切显示右下部分</onekit-view>
      <onekit-view onekit-class="page-section-ctn">
        <onekit-image onekit-class="image two-direction-fix" mode="bottom right" src="{{url}}"></onekit-image>
      </onekit-view>
    </onekit-view>
  </onekit-view>
</onekit-view>
