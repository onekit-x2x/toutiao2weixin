<video  
class="onekit-video {{onekitClass}}"
style="{{onekitStyle}}"
id="{{onekitId}}"
src="{{src}}"
autoplay="{{autoplay}}" 
poster="{{poster}}" 
loop="{{loop}}" 
show-fullscreen-btn="{{showFullscreenBtn}}" 
show-play-btn="{{showPlayBtn}}" 
controls="{{controls}}" 
object-fit="{{objectFit}}"
play-btn-position="{{playBtnPosition}}"
ad-unit-id="{{preRollUnitId}}"
animation="{{animation}}"

bindplay="video_play"
bindpause="video_pause"
bindended="video_ended"
binderror="video_error"
bindtimeupdate="video_timeupdate"
bindfullscreenchange="video_fullscreenchange"
/>
