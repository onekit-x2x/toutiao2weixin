<input
class="onekit-input {{onekitClass}}"
style="{{onekitStyle}}"
id="{{onekitId}}"
value="{{value}}"
type="{{type}}"
password="{{password}}"
placeholder="{{placeholder}}"
placeholder-style="{{placeholderStyle}}"
disabled="{{disabled}}"
maxlength="{{maxlength}}"
focus="{{focus}}"
cursor="{{cursor}}"
cursor-spacing="cursorSpacing"
selection-start="{{selectionStart}}"
selection-end="{{selectionEnd}}"

bindinput="input_input"
bindfocus="input_focus"
bindblur="input_blur"
bindconfirm="input_confirm"


bindtap="ui_tap"
bindtouchstart="ui_touchstart"
bindtouchmove="ui_touchmove"
bindtouchcancel="ui_touchcancel"
bindtouchend="ui_touchend"
bindlongpress="ui_longpress"
bindlongtap="ui_longtap"
bindtransitionend="ui_transitionend"
bindanimationstart="ui_animationstart"
bindanimationiteration="ui_animationiteration"
bindanimationend="ui_animationend"
bindtouchforcechange="ui_touchforcechange"
></input>