<radio
class="onekit-radio {{onekitClass}}"
style="{{onekitStyle}}"
id="{{onekitId}}"
value="{{value}}"
checked="{{checked}}"
disabled="{{disabled}}"
color="{{colors}}"

bindtap="item_tap"


catchtap="ui_tap"
catchtouchstart="ui_touchstart"
catchtouchmove="ui_touchmove"
catchtouchcancel="ui_touchcancel"
catchtouchend="ui_touchend"
catchlongpress="ui_longpress"
catchlongtap="ui_longtap"
catchtransitionend="ui_transitionend"
catchanimationstart="ui_animationstart"
catchanimationiteration="ui_animationiteration"
catchanimationend="ui_animationend"
catchtouchforcechange="ui_touchforcechange"
>

</radio>